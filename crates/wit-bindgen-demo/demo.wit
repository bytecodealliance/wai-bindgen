type files = list<tuple<string, string>>

enum lang {
  js,
  rust,
  java,
  wasmtime,
  wasmtime-py,
  c,
  markdown,
}


record options {
  rust-unchecked: bool,
  wasmtime-tracing: bool,
  js-compat: bool,
  js-instantiation: bool,
  is-import: bool,
}

render: func(lang: lang, wit: string, options: options) -> result<files, string>
